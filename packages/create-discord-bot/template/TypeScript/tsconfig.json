{
	"$schema": "https://json.schemastore.org/tsconfig.json",
	"extends": "@sapphire/ts-config/extra-strict",
	"compilerOptions": {
		"allowImportingTsExtensions": true,
		"allowSyntheticDefaultImports": true,
		"declaration": false,
		"declarationMap": false,
		"module": "ESNext",
		"moduleResolution": "NodeNext",
		"target": "ESNext",
		"outDir": "dist",
		"skipLibCheck": true
	}
}
